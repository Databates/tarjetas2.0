
  <form class="form-signin" name="input" action="/sign_up" method="post">
    <h2 class="form-signin-heading" style="text-align: center">Sign Up!</h2>
    <input type="text" class="form-control" placeholder="Username" name="user" required autofocus>
    <input type="password" class="form-control" placeholder="Password" name="password" required>
    <br>
    <button class="btn btn-lg btn-primary btn-block" type="submit">Create Account</button>
  </form>

